{
  "version": 3,
  "pages404": true,
  "caseSensitive": false,
  "basePath": "",
  "redirects": [
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "internal": true,
      "statusCode": 308,
      "regex": "^(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))/$"
    },
    {
      "source": "/legacy/:path*",
      "destination": "/:path*",
      "statusCode": 308,
      "regex": "^(?!/_next)/legacy(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?(?:/)?$"
    },
    {
      "source": "/home",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/home(?:/)?$"
    },
    {
      "source": "/index.html",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/index\\.html(?:/)?$"
    },
    {
      "source": "/home.html",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/home\\.html(?:/)?$"
    },
    {
      "source": "/deals.html",
      "destination": "/deals",
      "statusCode": 308,
      "regex": "^(?!/_next)/deals\\.html(?:/)?$"
    },
    {
      "source": "/blog.html",
      "destination": "/blog",
      "statusCode": 308,
      "regex": "^(?!/_next)/blog\\.html(?:/)?$"
    },
    {
      "source": "/about.html",
      "destination": "/about",
      "statusCode": 308,
      "regex": "^(?!/_next)/about\\.html(?:/)?$"
    },
    {
      "source": "/contact.html",
      "destination": "/contact",
      "statusCode": 308,
      "regex": "^(?!/_next)/contact\\.html(?:/)?$"
    },
    {
      "source": "/privacy.html",
      "destination": "/privacy",
      "statusCode": 308,
      "regex": "^(?!/_next)/privacy\\.html(?:/)?$"
    },
    {
      "source": "/terms-of-service.html",
      "destination": "/terms-of-service",
      "statusCode": 308,
      "regex": "^(?!/_next)/terms-of-service\\.html(?:/)?$"
    },
    {
      "source": "/affiliate-disclosure.html",
      "destination": "/affiliate-disclosure",
      "statusCode": 308,
      "regex": "^(?!/_next)/affiliate-disclosure\\.html(?:/)?$"
    },
    {
      "source": "/blog/:slug.html",
      "destination": "/blog/:slug",
      "statusCode": 308,
      "regex": "^(?!/_next)/blog(?:/([^/]+?))\\.html(?:/)?$"
    },
    {
      "source": "/pages/:path*.html",
      "destination": "/:path*",
      "statusCode": 308,
      "regex": "^(?!/_next)/pages(?:/((?:[^/]+?)(?:/(?:[^/]+?))*))?\\.html(?:/)?$"
    },
    {
      "source": "/index.php",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/index\\.php(?:/)?$"
    },
    {
      "source": "/home.php",
      "destination": "/",
      "statusCode": 308,
      "regex": "^(?!/_next)/home\\.php(?:/)?$"
    }
  ],
  "headers": [
    {
      "source": "/((?!api/).*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=300, s-maxage=3600, stale-while-revalidate=86400"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' https://www.googletagmanager.com https://www.google-analytics.com; style-src 'self' https://fonts.googleapis.com; font-src 'self' https://fonts.gstatic.com; img-src 'self' data: https: blob:; connect-src 'self' https://www.google-analytics.com https://vitals.vercel-analytics.com https://deal-aggregator-api-production.up.railway.app; frame-src 'self'; base-uri 'self'; form-action 'self'"
        },
        {
          "key": "Permissions-Policy",
          "value": "camera=(), microphone=(), geolocation=(), interest-cohort=()"
        }
      ],
      "regex": "^(?:/((?!api/).*))(?:/)?$"
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ],
      "regex": "^/_next/static(?:/(.*))(?:/)?$"
    },
    {
      "source": "/",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=60, s-maxage=300, stale-while-revalidate=3600"
        }
      ],
      "regex": "^/(?:/)?$"
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, max-age=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ],
      "regex": "^/api(?:/(.*))(?:/)?$"
    }
  ],
  "rewrites": {
    "beforeFiles": [],
    "afterFiles": [
      {
        "source": "/api/deals",
        "destination": "/api/deals?cache=true",
        "regex": "^/api/deals(?:/)?$"
      }
    ],
    "fallback": []
  },
  "dynamicRoutes": [
    {
      "page": "/blog/[slug]",
      "regex": "^/blog/([^/]+?)(?:/)?$",
      "routeKeys": {
        "nxtPslug": "nxtPslug"
      },
      "namedRegex": "^/blog/(?<nxtPslug>[^/]+?)(?:/)?$"
    }
  ],
  "staticRoutes": [
    {
      "page": "/",
      "regex": "^/(?:/)?$",
      "routeKeys": {},
