{
  "redirects": [
    { "source": "http://dealradarus.com/(.*)", "destination": "https://dealradarus.com/$1", "permanent": true },
    { "source": "https://www.dealradarus.com/(.*)", "destination": "https://dealradarus.com/$1", "permanent": true }
  ],
  "headers": [
    { "source": "/(.*)", "headers": [
      { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
      { "key": "X-Content-Type-Options", "value": "nosniff" },
      { "key": "Referrer-Policy", "value": "strict-origin-when-cross-origin" }
    ]}
  ]
}